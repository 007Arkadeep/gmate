<style-scheme id="barf" _name="barf" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#3e4d5c"/>
  <style name="text"              foreground="#eef2f7" background="#15191e"/>
  <style name="cursor"            foreground="#c4c4c4"/>
  <style name="current-line"      background="#191d22"/>
  <style name="line-numbers"      foreground="#eef2f7" background="#15191e"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#6e6e6e" italic="true"/>
  <style name="def:shebang" foreground="#6e6e6e" italic="true"/>
  <style name="def:doc-comment-element" foreground="#6e6e6e" italic="true"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#708e67"/>
  <style name="def:string" foreground="#5c81b3"/>
  <style name="def:character" foreground="#5c81b3"/>
  <style name="def:special-char" foreground="#eef2f7"/>
  <style name="def:special-constant" foreground="#53667d"/>
  <style name="def:instance-variable" foreground="#708e67"/>
  <style name="def:boolean" foreground="#53667d"/>
  <style name="def:number" foreground="#c1e1b8"/>
  <style name="def:base-n-integer" foreground="#c1e1b8"/>
  <style name="def:decimal" foreground="#c1e1b8"/>
  <style name="def:complex" foreground="#c1e1b8"/>
  <style name="def:floating-point" foreground="#c1e1b8"/>
  <style name="def:keyword" foreground="#697a8e" bold="true"/>
  <style name="def:builtin"  background="#282e36"/>
  <style name="def:variable" foreground="#708e67"/>
  <style name="def:class-variable" foreground="#708e67"/>
  <style name="def:identifier" foreground="#708e67"/>
  <style name="def:statement" foreground="#708e67"/>
  <style name="def:type" foreground="#708e67"/>
  <style name="def:error" background="#C80000" foreground="#F0EA20"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#708e67"/>
  <style name="def:preprocessor" foreground="#697a8e" bold="true"/>
  <style name="def:specials" foreground="#708e67"/>
  <!--style name="def:reserved"  background="#C80000" foreground="#F0EA20"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#708e67"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#708e67"/>
  <style name="ruby:regex" foreground="#5c81b3"/>
  <style name="ruby:class-definition" foreground="#708e67"/>
  <style name="ruby:simple-interpolation" foreground="#53667d"/>
  <style name="ruby:complex-interpolation" foreground="#53667d"/>
  <style name="ruby:function-definition"  background="#282e36"/>
  <style name="ruby:special-variable" foreground="#708e67"/>
  <style name="ruby:predefined-variable" />
  <style name="ruby:global-variable" foreground="#708e67"/>
  <style name="ruby:instance-variable" foreground="#708e67"/>
  <style name="ruby:class-variable" foreground="#708e67"/>
  <style name="ruby:operators" foreground="#697a8e" bold="true"/>
  <style name="ruby:symbol" />

  <!-- YAML Styles -->
  <style name="yaml:inner-key" />
  <style name="yaml:scalar-value" foreground="#5c81b3"/>

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#708e67"/>
  <style name="rubyonrails:regex" foreground="#5c81b3"/>
  <style name="rubyonrails:class-definition" foreground="#708e67"/>
  <style name="rubyonrails:simple-interpolation" foreground="#53667d"/>
  <style name="rubyonrails:complex-interpolation" foreground="#53667d"/>
  <style name="rubyonrails:function-definition"  background="#282e36"/>
  <style name="rubyonrails:special-variable" foreground="#708e67"/>
  <style name="rubyonrails:predefined-variable" />
  <style name="rubyonrails:global-variable" foreground="#708e67"/>
  <style name="rubyonrails:instance-variable" foreground="#708e67"/>
  <style name="rubyonrails:class-variable" foreground="#708e67"/>
  <style name="rubyonrails:operators" foreground="#697a8e" bold="true"/>
  <style name="rubyonrails:symbol" />
  <style name="rubyonrails:rails" foreground="#708e67"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" />
  <style name="diff:removed-line" />
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" />
  <style name="diff:diff-file" foreground="#697a8e" bold="true"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#708e67"/>
  <style name="css:known-property-values" />
  <style name="css:others-2" foreground="#708e67"/>
  <style name="css:others-3"  foreground="#708e67"/>

  <!-- XML Styles -->
  <style name="xml:attribute-name"  background="#282e36"/>
  <style name="xml:element-name" />
  <style name="xml:attribute-value" foreground="#5c81b3"/>
  <style name="xml:namespace" />
  <style name="xml:entity" foreground="#708e67"/>
  <style name="html:script" foreground="#708e67"/>

</style-scheme>


<style-scheme id="spacecadet" _name="SpaceCadet" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#40002f"/>
  <style name="text"              foreground="#dde6cf" background="#0d0d0d"/>
  <style name="cursor"            foreground="#7f005d"/>
  <style name="current-line"      background="#0c0c0c"/>
  <style name="line-numbers"      foreground="#dde6cf" background="#0d0d0d"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#473c45"/>
  <style name="def:shebang" foreground="#473c45"/>
  <style name="def:doc-comment-element" foreground="#473c45"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#a8885a"/>
  <style name="def:string" foreground="#805978"/>
  <style name="def:character" foreground="#805978"/>
  <style name="def:special-char" foreground="#dde6cf"/>
  <style name="def:special-constant" foreground="#a8885a"/>
  <style name="def:instance-variable" foreground="#596380"/>
  <style name="def:boolean" foreground="#a8885a"/>
  <style name="def:number" foreground="#a8885a"/>
  <style name="def:base-n-integer" foreground="#a8885a"/>
  <style name="def:decimal" foreground="#a8885a"/>
  <style name="def:complex" foreground="#a8885a"/>
  <style name="def:floating-point" foreground="#a8885a"/>
  <style name="def:keyword" />
  <style name="def:builtin" foreground="#6078bf"/>
  <style name="def:variable" foreground="#596380"/>
  <style name="def:class-variable" foreground="#596380"/>
  <style name="def:identifier" foreground="#596380"/>
  <style name="def:statement" foreground="#a8885a"/>
  <style name="def:type" foreground="#a8885a"/>
  <style name="def:error"  background="#5f0047"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#596380"/>
  <style name="def:preprocessor" />
  <style name="def:specials" foreground="#6078bf"/>
  <!--style name="def:reserved"   background="#5f0047"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#6078bf"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#a8885a"/>
  <style name="ruby:regex" foreground="#805978"/>
  <style name="ruby:class-definition" foreground="#6078bf"/>
  <style name="ruby:simple-interpolation" foreground="#805978"/>
  <style name="ruby:complex-interpolation" foreground="#805978"/>
  <style name="ruby:function-definition" foreground="#6078bf"/>
  <style name="ruby:special-variable" foreground="#596380"/>
  <style name="ruby:predefined-variable" foreground="#6078bf"/>
  <style name="ruby:global-variable" foreground="#596380"/>
  <style name="ruby:instance-variable" foreground="#596380"/>
  <style name="ruby:class-variable" foreground="#596380"/>
  <style name="ruby:operators" />
  <style name="ruby:symbol" foreground="#a8885a"/>

  <!-- YAML Styles -->
  <style name="yaml:inner-key" foreground="#a8885a"/>
  <style name="yaml:scalar-value" foreground="#805978"/>

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#a8885a"/>
  <style name="rubyonrails:regex" foreground="#805978"/>
  <style name="rubyonrails:class-definition" foreground="#6078bf"/>
  <style name="rubyonrails:simple-interpolation" foreground="#805978"/>
  <style name="rubyonrails:complex-interpolation" foreground="#805978"/>
  <style name="rubyonrails:function-definition" foreground="#6078bf"/>
  <style name="rubyonrails:special-variable" foreground="#596380"/>
  <style name="rubyonrails:predefined-variable" foreground="#6078bf"/>
  <style name="rubyonrails:global-variable" foreground="#596380"/>
  <style name="rubyonrails:instance-variable" foreground="#596380"/>
  <style name="rubyonrails:class-variable" foreground="#596380"/>
  <style name="rubyonrails:operators" />
  <style name="rubyonrails:symbol" foreground="#a8885a"/>
  <style name="rubyonrails:rails" foreground="#8a4b66"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" />
  <style name="diff:removed-line" />
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" foreground="#a8885a"/>
  <style name="diff:diff-file" />
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#596380"/>
  <style name="css:known-property-values" foreground="#a8885a"/>
  <style name="css:others-2" foreground="#6078bf"/>
  <style name="css:others-3"  foreground="#6078bf"/>

  <!-- XML Styles -->
  <style name="xml:element-name" foreground="#6078bf"/>
  <style name="xml:attribute-name" foreground="#6078bf"/>
  <style name="xml:attribute-value" foreground="#805978"/>
  <style name="xml:processing-instruction" foreground="#6078bf"/>
  <style name="xml:namespace" foreground="#a8885a"/>
  <style name="xml:entity" foreground="#a8885a"/>
  <style name="html:script" foreground="#6078bf"/>

</style-scheme>


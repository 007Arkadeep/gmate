<style-scheme id="espressolibre" _name="Espresso Libre" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#c3dcff"/>
  <style name="text"              foreground="#bdae9d" background="#2a211c"/>
  <style name="cursor"            foreground="#889aff"/>
  <style name="current-line"      background="#3a312c"/>
  <style name="line-numbers"      foreground="#bdae9d" background="#2a211c"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#0066ff" italic="true"/>
  <style name="def:shebang" foreground="#0066ff" italic="true"/>
  <style name="def:doc-comment-element" foreground="#0066ff" italic="true"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#318495"/>
  <style name="def:string" foreground="#049b0a"/>
  <style name="def:character" foreground="#049b0a"/>
  <style name="def:special-char" foreground="#bdae9d"/>
  <style name="def:special-constant" foreground="#585cf6" bold="true"/>
  <style name="def:instance-variable" foreground="#318495"/>
  <style name="def:boolean" foreground="#585cf6" bold="true"/>
  <style name="def:number" foreground="#44aa43"/>
  <style name="def:base-n-integer" foreground="#44aa43"/>
  <style name="def:decimal" foreground="#44aa43"/>
  <style name="def:complex" foreground="#44aa43"/>
  <style name="def:floating-point" foreground="#44aa43"/>
  <style name="def:keyword" foreground="#43a8ed" bold="true"/>
  <style name="def:builtin" foreground="#ff9358" bold="true"/>
  <style name="def:variable" foreground="#318495"/>
  <style name="def:class-variable" foreground="#318495"/>
  <style name="def:identifier" foreground="#318495"/>
  <style name="def:statement" foreground="#318495"/>
  <style name="def:type" foreground="#318495"/>
  <style name="def:error" background="#C80000" foreground="#F0EA20"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#318495"/>
  <style name="def:preprocessor" foreground="#43a8ed" bold="true"/>
  <style name="def:specials" foreground="#318495"/>
  <!--style name="def:reserved"  background="#C80000" foreground="#F0EA20"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#318495"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#318495"/>
  <style name="ruby:regex" foreground="#049b0a"/>
  <style name="ruby:class-definition" foreground="#318495"/>
  <style name="ruby:simple-interpolation" foreground="#c5656b" bold="true"/>
  <style name="ruby:complex-interpolation" foreground="#c5656b" bold="true"/>
  <style name="ruby:function-definition" foreground="#ff9358" bold="true"/>
  <style name="ruby:special-variable" foreground="#318495"/>
  <style name="ruby:predefined-variable" foreground="#43a8ed"/>
  <style name="ruby:global-variable" foreground="#318495"/>
  <style name="ruby:instance-variable" foreground="#318495"/>
  <style name="ruby:class-variable" foreground="#318495"/>
  <style name="ruby:operators" foreground="#43a8ed" bold="true"/>
  <style name="ruby:symbol" foreground="#c5656b" bold="true"/>

  <!-- YAML Styles -->
  <style name="yaml:inner-key" foreground="#c5656b" bold="true"/>
  <style name="yaml:scalar-value" foreground="#049b0a"/>

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#318495"/>
  <style name="rubyonrails:regex" foreground="#049b0a"/>
  <style name="rubyonrails:class-definition" foreground="#318495"/>
  <style name="rubyonrails:simple-interpolation" foreground="#c5656b" bold="true"/>
  <style name="rubyonrails:complex-interpolation" foreground="#c5656b" bold="true"/>
  <style name="rubyonrails:function-definition" foreground="#ff9358" bold="true"/>
  <style name="rubyonrails:special-variable" foreground="#318495"/>
  <style name="rubyonrails:predefined-variable" foreground="#43a8ed"/>
  <style name="rubyonrails:global-variable" foreground="#318495"/>
  <style name="rubyonrails:instance-variable" foreground="#318495"/>
  <style name="rubyonrails:class-variable" foreground="#318495"/>
  <style name="rubyonrails:operators" foreground="#43a8ed" bold="true"/>
  <style name="rubyonrails:symbol" foreground="#c5656b" bold="true"/>
  <style name="rubyonrails:rails" foreground="#318495"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" />
  <style name="diff:removed-line" />
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" foreground="#c5656b" bold="true"/>
  <style name="diff:diff-file" foreground="#43a8ed" bold="true"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#318495"/>
  <style name="css:known-property-values" foreground="#c5656b" bold="true"/>
  <style name="css:others-2" foreground="#318495"/>
  <style name="css:others-3"  foreground="#318495"/>

  <!-- XML Styles -->
  <style name="xml:attribute-name" foreground="#ff9358" bold="true"/>
  <style name="xml:element-name" foreground="#43a8ed"/>
  <style name="xml:attribute-value" foreground="#049b0a"/>
  <style name="xml:namespace" foreground="#c5656b" bold="true"/>
  <style name="xml:entity" foreground="#318495"/>
  <style name="html:script" foreground="#318495"/>

</style-scheme>


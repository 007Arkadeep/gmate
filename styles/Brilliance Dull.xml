<style-scheme id="brilliancedull" _name="Brilliance Dull" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#111148"/>
  <style name="text"              foreground="#cdcdcd" background="#050505"/>
  <style name="cursor"            foreground="#7979b7"/>
  <style name="current-line"      background="#040411"/>
  <style name="line-numbers"      foreground="#cdcdcd" background="#050505"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#333333"/>
  <style name="def:shebang" foreground="#333333"/>
  <style name="def:doc-comment-element" foreground="#333333"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#80a659"/>
  <style name="def:string" />
  <style name="def:character" />
  <style name="def:special-char" foreground="#cdcdcd"/>
  <style name="def:special-constant" foreground="#59a559"/>
  <style name="def:instance-variable" foreground="#5780a6"/>
  <style name="def:boolean" foreground="#59a559"/>
  <style name="def:number" foreground="#95a658"/>
  <style name="def:base-n-integer" foreground="#95a658"/>
  <style name="def:decimal" foreground="#95a658"/>
  <style name="def:complex" foreground="#95a658"/>
  <style name="def:floating-point" foreground="#95a658"/>
  <style name="def:keyword" foreground="#a459a5" background="#10041d"/>
  <style name="def:builtin" foreground="#a75980"/>
  <style name="def:variable" foreground="#5780a6"/>
  <style name="def:class-variable" foreground="#5780a6"/>
  <style name="def:identifier" foreground="#5780a6"/>
  <style name="def:statement" foreground="#80a659"/>
  <style name="def:type" foreground="#80a659"/>
  <style name="def:error" foreground="#211211" background="#a7595a" bold="true"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#5780a6"/>
  <style name="def:preprocessor" foreground="#a459a5" background="#10041d"/>
  <style name="def:specials" foreground="#80a659"/>
  <!--style name="def:reserved"  foreground="#211211" background="#a7595a" bold="true"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#80a659"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#80a659"/>
  <style name="ruby:regex" foreground="#a6a458" background="#111d04"/>
  <style name="ruby:class-definition" foreground="#80a659"/>
  <style name="ruby:simple-interpolation" foreground="#80a659" background="#1a1a1a"/>
  <style name="ruby:complex-interpolation" foreground="#80a659" background="#1a1a1a"/>
  <style name="ruby:function-definition" foreground="#a75980"/>
  <style name="ruby:special-variable" foreground="#5780a6"/>
  <style name="ruby:predefined-variable" foreground="#333333"/>
  <style name="ruby:global-variable" foreground="#5780a6"/>
  <style name="ruby:instance-variable" foreground="#5780a6"/>
  <style name="ruby:class-variable" foreground="#5780a6"/>
  <style name="ruby:operators" foreground="#a459a5" background="#10041d"/>
  <style name="ruby:symbol" foreground="#57a57d"/>

  <!-- YAML Styles -->
  <style name="yaml:inner-key" foreground="#57a57d"/>
  <style name="yaml:scalar-value" />

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#80a659"/>
  <style name="rubyonrails:regex" foreground="#a6a458" background="#111d04"/>
  <style name="rubyonrails:class-definition" foreground="#80a659"/>
  <style name="rubyonrails:simple-interpolation" foreground="#80a659" background="#1a1a1a"/>
  <style name="rubyonrails:complex-interpolation" foreground="#80a659" background="#1a1a1a"/>
  <style name="rubyonrails:function-definition" foreground="#a75980"/>
  <style name="rubyonrails:special-variable" foreground="#5780a6"/>
  <style name="rubyonrails:predefined-variable" foreground="#333333"/>
  <style name="rubyonrails:global-variable" foreground="#5780a6"/>
  <style name="rubyonrails:instance-variable" foreground="#5780a6"/>
  <style name="rubyonrails:class-variable" foreground="#5780a6"/>
  <style name="rubyonrails:operators" foreground="#a459a5" background="#10041d"/>
  <style name="rubyonrails:symbol" foreground="#57a57d"/>
  <style name="rubyonrails:rails" foreground="#80a659"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" foreground="#81bb9e" background="#15281f"/>
  <style name="diff:removed-line" foreground="#bc839f" background="#400021"/>
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" foreground="#57a57d"/>
  <style name="diff:diff-file" foreground="#a459a5" background="#10041d"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#5780a6"/>
  <style name="css:known-property-values" foreground="#57a57d"/>
  <style name="css:others-2" foreground="#80a659"/>
  <style name="css:others-3"  foreground="#80a659"/>

  <!-- XML Styles -->
  <style name="xml:element-name" foreground="#505050"/>
  <style name="xml:attribute-name" foreground="#373737"/>
  <style name="xml:attribute-value" />
  <style name="xml:processing-instruction" foreground="#333333"/>
  <style name="xml:namespace" foreground="#57a57d"/>
  <style name="xml:entity" foreground="#80a659"/>
  <style name="html:script" foreground="#80a659"/>

</style-scheme>


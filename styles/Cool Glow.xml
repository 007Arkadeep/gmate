<style-scheme id="coolglow" _name="Cool Glow" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#122bbb"/>
  <style name="text"              foreground="#e0e0e0" background="#06071d"/>
  <style name="cursor"            foreground="#ffffff"/>
  <style name="current-line"      background="#14152a"/>
  <style name="line-numbers"      foreground="#e0e0e0" background="#06071d"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#aeaeae"/>
  <style name="def:shebang" foreground="#aeaeae"/>
  <style name="def:doc-comment-element" foreground="#aeaeae"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#62e9bd"/>
  <style name="def:string" foreground="#8dff8e"/>
  <style name="def:character" foreground="#8dff8e"/>
  <style name="def:special-char" foreground="#e0e0e0"/>
  <style name="def:special-constant" foreground="#62e9bd"/>
  <style name="def:instance-variable" foreground="#b683ca"/>
  <style name="def:boolean" foreground="#62e9bd"/>
  <style name="def:number" foreground="#62e9bd"/>
  <style name="def:base-n-integer" foreground="#62e9bd"/>
  <style name="def:decimal" foreground="#62e9bd"/>
  <style name="def:complex" foreground="#62e9bd"/>
  <style name="def:floating-point" foreground="#62e9bd"/>
  <style name="def:keyword" foreground="#2bf1dc"/>
  <style name="def:builtin" foreground="#a3ebff"/>
  <style name="def:variable" foreground="#b683ca"/>
  <style name="def:class-variable" foreground="#b683ca"/>
  <style name="def:identifier" foreground="#b683ca"/>
  <style name="def:statement" foreground="#62e9bd"/>
  <style name="def:type" foreground="#62e9bd"/>
  <style name="def:error" foreground="#f8f8f8" background="#ad2117"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#b683ca"/>
  <style name="def:preprocessor" foreground="#2bf1dc"/>
  <style name="def:specials" foreground="#a3ebff"/>
  <!--style name="def:reserved"  foreground="#f8f8f8" background="#ad2117"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#a3ebff"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#62e9bd"/>
  <style name="ruby:regex" foreground="#8dff8e"/>
  <style name="ruby:class-definition" foreground="#a3ebff"/>
  <style name="ruby:simple-interpolation" foreground="#92ffa3"/>
  <style name="ruby:complex-interpolation" foreground="#92ffa3"/>
  <style name="ruby:function-definition" foreground="#a3ebff"/>
  <style name="ruby:special-variable" foreground="#b683ca"/>
  <style name="ruby:predefined-variable" foreground="#7bacca"/>
  <style name="ruby:global-variable" foreground="#b683ca"/>
  <style name="ruby:instance-variable" foreground="#b683ca"/>
  <style name="ruby:class-variable" foreground="#b683ca"/>
  <style name="ruby:operators" foreground="#2bf1dc"/>
  <style name="ruby:symbol" foreground="#62e9bd"/>

  <!-- YAML Styles -->
  <style name="yaml:inner-key" foreground="#62e9bd"/>
  <style name="yaml:scalar-value" foreground="#8dff8e"/>

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#62e9bd"/>
  <style name="rubyonrails:regex" foreground="#8dff8e"/>
  <style name="rubyonrails:class-definition" foreground="#a3ebff"/>
  <style name="rubyonrails:simple-interpolation" foreground="#92ffa3"/>
  <style name="rubyonrails:complex-interpolation" foreground="#92ffa3"/>
  <style name="rubyonrails:function-definition" foreground="#a3ebff"/>
  <style name="rubyonrails:special-variable" foreground="#b683ca"/>
  <style name="rubyonrails:predefined-variable" foreground="#7bacca"/>
  <style name="rubyonrails:global-variable" foreground="#b683ca"/>
  <style name="rubyonrails:instance-variable" foreground="#b683ca"/>
  <style name="rubyonrails:class-variable" foreground="#b683ca"/>
  <style name="rubyonrails:operators" foreground="#2bf1dc"/>
  <style name="rubyonrails:symbol" foreground="#62e9bd"/>
  <style name="rubyonrails:rails" foreground="#60a4f1"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" />
  <style name="diff:removed-line" />
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" foreground="#62e9bd"/>
  <style name="diff:diff-file" foreground="#2bf1dc"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#b683ca"/>
  <style name="css:known-property-values" foreground="#62e9bd"/>
  <style name="css:others-2" foreground="#a3ebff"/>
  <style name="css:others-3"  foreground="#a3ebff"/>

  <!-- XML Styles -->
  <style name="xml:element-name" foreground="#7bacca"/>
  <style name="xml:attribute-name" foreground="#a3ebff"/>
  <style name="xml:attribute-value" foreground="#8dff8e"/>
  <style name="xml:processing-instruction" foreground="#7bacca"/>
  <style name="xml:namespace" foreground="#62e9bd"/>
  <style name="xml:entity" foreground="#62e9bd"/>
  <style name="html:script" foreground="#a3ebff"/>

</style-scheme>


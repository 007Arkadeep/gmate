<style-scheme id="magicwbamiga" _name="MagicWB (Amiga)" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#b1b1b1"/>
  <style name="text"              foreground="#000000" background="#969696"/>
  <style name="cursor"            foreground="#ffffff"/>
  <style name="current-line"      background="#8b8b8b"/>
  <style name="line-numbers"      foreground="#000000" background="#969696"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#8d2e75" italic="true"/>
  <style name="def:shebang" foreground="#8d2e75" italic="true"/>
  <style name="def:doc-comment-element" foreground="#8d2e75" italic="true"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#ffa995" background="#7878ab"/>
  <style name="def:string" foreground="#ffffff" background="#ab7878"/>
  <style name="def:character" foreground="#ffffff" background="#ab7878"/>
  <style name="def:special-char" foreground="#000000"/>
  <style name="def:special-constant" foreground="#ffa995" bold="true"/>
  <style name="def:instance-variable" foreground="#ffa995"/>
  <style name="def:boolean" foreground="#ffa995" bold="true"/>
  <style name="def:number" foreground="#ffffff"/>
  <style name="def:base-n-integer" foreground="#ffffff"/>
  <style name="def:decimal" foreground="#ffffff"/>
  <style name="def:complex" foreground="#ffffff"/>
  <style name="def:floating-point" foreground="#ffffff"/>
  <style name="def:keyword"  bold="true"/>
  <style name="def:builtin" foreground="#ffa995"/>
  <style name="def:variable" foreground="#ffa995"/>
  <style name="def:class-variable" foreground="#ffa995"/>
  <style name="def:identifier" foreground="#ffa995"/>
  <style name="def:statement" foreground="#ffa995" background="#7878ab"/>
  <style name="def:type" foreground="#ffa995" background="#7878ab"/>
  <style name="def:error" foreground="#ffffff" background="#797979"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#ffa995"/>
  <style name="def:preprocessor"  bold="true"/>
  <style name="def:specials" foreground="#0000ff" bold="true"/>
  <!--style name="def:reserved"  foreground="#ffffff" background="#797979"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#0000ff" bold="true"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#ffa995" background="#7878ab"/>
  <style name="ruby:regex" foreground="#ffffff" background="#ab7878"/>
  <style name="ruby:class-definition" foreground="#0000ff" bold="true"/>
  <style name="ruby:simple-interpolation" foreground="#ffffff" background="#ab7878"/>
  <style name="ruby:complex-interpolation" foreground="#ffffff" background="#ab7878"/>
  <style name="ruby:function-definition" foreground="#ffa995"/>
  <style name="ruby:special-variable" foreground="#ffa995"/>
  <style name="ruby:predefined-variable" foreground="#ffa995"/>
  <style name="ruby:global-variable" foreground="#ffa995"/>
  <style name="ruby:instance-variable" foreground="#ffa995"/>
  <style name="ruby:class-variable" foreground="#ffa995"/>
  <style name="ruby:operators"  bold="true"/>
  <style name="ruby:symbol" foreground="#ffa995" background="#7878ab"/>

  <!-- YAML Styles -->
  <style name="yaml:inner-key" foreground="#ffa995" background="#7878ab"/>
  <style name="yaml:scalar-value" foreground="#ffffff" background="#ab7878"/>

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#ffa995" background="#7878ab"/>
  <style name="rubyonrails:regex" foreground="#ffffff" background="#ab7878"/>
  <style name="rubyonrails:class-definition" foreground="#0000ff" bold="true"/>
  <style name="rubyonrails:simple-interpolation" foreground="#ffffff" background="#ab7878"/>
  <style name="rubyonrails:complex-interpolation" foreground="#ffffff" background="#ab7878"/>
  <style name="rubyonrails:function-definition" foreground="#ffa995"/>
  <style name="rubyonrails:special-variable" foreground="#ffa995"/>
  <style name="rubyonrails:predefined-variable" foreground="#ffa995"/>
  <style name="rubyonrails:global-variable" foreground="#ffa995"/>
  <style name="rubyonrails:instance-variable" foreground="#ffa995"/>
  <style name="rubyonrails:class-variable" foreground="#ffa995"/>
  <style name="rubyonrails:operators"  bold="true"/>
  <style name="rubyonrails:symbol" foreground="#ffa995" background="#7878ab"/>
  <style name="rubyonrails:rails" foreground="#e5b3ff"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" />
  <style name="diff:removed-line" />
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" foreground="#ffa995" background="#7878ab"/>
  <style name="diff:diff-file"  bold="true"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#ffa995"/>
  <style name="css:known-property-values" foreground="#ffa995" background="#7878ab"/>
  <style name="css:others-2" foreground="#0000ff" bold="true"/>
  <style name="css:others-3"  foreground="#0000ff" bold="true"/>

  <!-- XML Styles -->
  <style name="xml:element-name" foreground="#0000ff" bold="true"/>
  <style name="xml:attribute-name" foreground="#3a68a3" italic="true"/>
  <style name="xml:attribute-value" foreground="#ffffff" background="#ab7878"/>
  <style name="xml:processing-instruction" foreground="#ffa995"/>
  <style name="xml:namespace" foreground="#ffa995" background="#7878ab"/>
  <style name="xml:entity" foreground="#ffa995" background="#7878ab"/>
  <style name="html:script" foreground="#0000ff" bold="true"/>

</style-scheme>


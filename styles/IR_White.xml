<style-scheme id="irwhite" _name="IR_White" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#e0e0ed"/>
  <style name="text"              foreground="#010101" background="#ffffff"/>
  <style name="cursor"            foreground="#a7a7a7"/>
  <style name="current-line"      background="#ffffff"/>
  <style name="line-numbers"      foreground="#010101" background="#ffffff"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#898989"/>
  <style name="def:shebang" foreground="#898989"/>
  <style name="def:doc-comment-element" foreground="#898989"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#333366"/>
  <style name="def:string" foreground="#009f78"/>
  <style name="def:character" foreground="#009f78"/>
  <style name="def:special-char" foreground="#010101"/>
  <style name="def:special-constant" foreground="#333366"/>
  <style name="def:instance-variable" foreground="#696989"/>
  <style name="def:boolean" foreground="#333366"/>
  <style name="def:number" foreground="#8c008a"/>
  <style name="def:base-n-integer" foreground="#8c008a"/>
  <style name="def:decimal" foreground="#8c008a"/>
  <style name="def:complex" foreground="#8c008a"/>
  <style name="def:floating-point" foreground="#8c008a"/>
  <style name="def:keyword" foreground="#016692"/>
  <style name="def:builtin" foreground="#a15001"/>
  <style name="def:variable" foreground="#696989"/>
  <style name="def:class-variable" foreground="#696989"/>
  <style name="def:identifier" foreground="#696989"/>
  <style name="def:statement" foreground="#333366"/>
  <style name="def:type" foreground="#333366"/>
  <style name="def:error" foreground="#a00294" background="#e78de2"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#696989"/>
  <style name="def:preprocessor" foreground="#016692"/>
  <style name="def:specials" foreground="#a15001"/>
  <!--style name="def:reserved"  foreground="#a00294" background="#e78de2"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#a15001"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#333366"/>
  <style name="ruby:regex" foreground="#9d7416"/>
  <style name="ruby:class-definition" foreground="#a15001"/>
  <style name="ruby:simple-interpolation" foreground="#00ff00"/>
  <style name="ruby:complex-interpolation" foreground="#00ff00"/>
  <style name="ruby:function-definition" foreground="#a15001"/>
  <style name="ruby:special-variable" foreground="#696989"/>
  <style name="ruby:predefined-variable" foreground="#0067c2"/>
  <style name="ruby:global-variable" foreground="#696989"/>
  <style name="ruby:instance-variable" foreground="#696989"/>
  <style name="ruby:class-variable" foreground="#696989"/>
  <style name="ruby:operators" foreground="#016692"/>
  <style name="ruby:symbol" foreground="#333366"/>

  <!-- YAML Styles -->
  <style name="yaml:inner-key" foreground="#333366"/>
  <style name="yaml:scalar-value" foreground="#009f78"/>

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#333366"/>
  <style name="rubyonrails:regex" foreground="#9d7416"/>
  <style name="rubyonrails:class-definition" foreground="#a15001"/>
  <style name="rubyonrails:simple-interpolation" foreground="#00ff00"/>
  <style name="rubyonrails:complex-interpolation" foreground="#00ff00"/>
  <style name="rubyonrails:function-definition" foreground="#a15001"/>
  <style name="rubyonrails:special-variable" foreground="#696989"/>
  <style name="rubyonrails:predefined-variable" foreground="#0067c2"/>
  <style name="rubyonrails:global-variable" foreground="#696989"/>
  <style name="rubyonrails:instance-variable" foreground="#696989"/>
  <style name="rubyonrails:class-variable" foreground="#696989"/>
  <style name="rubyonrails:operators" foreground="#016692"/>
  <style name="rubyonrails:symbol" foreground="#333366"/>
  <style name="rubyonrails:rails" foreground="#646409"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" foreground="#ffffff" background="#253b22"/>
  <style name="diff:removed-line" foreground="#ffffff" background="#420e09"/>
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" foreground="#333366"/>
  <style name="diff:diff-file" foreground="#016692"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#696989"/>
  <style name="css:known-property-values" foreground="#333366"/>
  <style name="css:others-2" foreground="#a15001"/>
  <style name="css:others-3"  foreground="#a15001"/>

  <!-- XML Styles -->
  <style name="xml:element-name" foreground="#0067c2"/>
  <style name="xml:attribute-name" foreground="#bc4d00"/>
  <style name="xml:attribute-value" foreground="#009f78"/>
  <style name="xml:processing-instruction" foreground="#0067c2"/>
  <style name="xml:namespace" foreground="#333366"/>
  <style name="xml:entity" foreground="#333366"/>
  <style name="html:script" foreground="#a15001"/>

</style-scheme>


<style-scheme id="ilife05" _name="iLife 05" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#e8bffc"/>
  <style name="text"              foreground="#333333" background="#ffffff"/>
  <style name="cursor"            foreground="#a600f6"/>
  <style name="current-line"      background="#ffffed"/>
  <style name="line-numbers"      foreground="#333333" background="#ffffff"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#b35eed" background="#f7f7f7" italic="true"/>
  <style name="def:shebang" foreground="#b35eed" background="#f7f7f7" italic="true"/>
  <style name="def:doc-comment-element" foreground="#b35eed" background="#f7f7f7" italic="true"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#308095" bold="true"/>
  <style name="def:string" foreground="#69b200" background="#fffef8"/>
  <style name="def:character" foreground="#69b200" background="#fffef8"/>
  <style name="def:special-char" foreground="#333333"/>
  <style name="def:special-constant" foreground="#1c7a1f" bold="true"/>
  <style name="def:instance-variable" foreground="#308095" bold="true"/>
  <style name="def:boolean" foreground="#1c7a1f" bold="true"/>
  <style name="def:number" foreground="#69b200" bold="true"/>
  <style name="def:base-n-integer" foreground="#69b200" bold="true"/>
  <style name="def:decimal" foreground="#69b200" bold="true"/>
  <style name="def:complex" foreground="#69b200" bold="true"/>
  <style name="def:floating-point" foreground="#69b200" bold="true"/>
  <style name="def:keyword" foreground="#000000" background="#fefcfd" bold="true"/>
  <style name="def:builtin" foreground="#ffffff" background="#69b200" bold="true"/>
  <style name="def:variable" foreground="#308095" bold="true"/>
  <style name="def:class-variable" foreground="#308095" bold="true"/>
  <style name="def:identifier" foreground="#308095" bold="true"/>
  <style name="def:statement" foreground="#308095" bold="true"/>
  <style name="def:type" foreground="#308095" bold="true"/>
  <style name="def:error" background="#C80000" foreground="#F0EA20"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#308095" bold="true"/>
  <style name="def:preprocessor" foreground="#000000" background="#fefcfd" bold="true"/>
  <style name="def:specials" foreground="#308095" bold="true"/>
  <!--style name="def:reserved"  background="#C80000" foreground="#F0EA20"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#308095" bold="true"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#308095" bold="true"/>
  <style name="ruby:regex" foreground="#69b200" background="#fffef8"/>
  <style name="ruby:class-definition" foreground="#308095" bold="true"/>
  <style name="ruby:simple-interpolation" foreground="#1c7a1f" background="#f4f8ee"/>
  <style name="ruby:complex-interpolation" foreground="#1c7a1f" background="#f4f8ee"/>
  <style name="ruby:function-definition" foreground="#ffffff" background="#69b200" bold="true"/>
  <style name="ruby:special-variable" foreground="#308095" bold="true"/>
  <style name="ruby:predefined-variable" foreground="#39a0ba" background="#eaf5f7"/>
  <style name="ruby:global-variable" foreground="#308095" bold="true"/>
  <style name="ruby:instance-variable" foreground="#308095" bold="true"/>
  <style name="ruby:class-variable" foreground="#308095" bold="true"/>
  <style name="ruby:operators" foreground="#000000" background="#fefcfd" bold="true"/>
  <style name="ruby:symbol" />

  <!-- YAML Styles -->
  <style name="yaml:inner-key" />
  <style name="yaml:scalar-value" foreground="#69b200" background="#fffef8"/>

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#308095" bold="true"/>
  <style name="rubyonrails:regex" foreground="#69b200" background="#fffef8"/>
  <style name="rubyonrails:class-definition" foreground="#308095" bold="true"/>
  <style name="rubyonrails:simple-interpolation" foreground="#1c7a1f" background="#f4f8ee"/>
  <style name="rubyonrails:complex-interpolation" foreground="#1c7a1f" background="#f4f8ee"/>
  <style name="rubyonrails:function-definition" foreground="#ffffff" background="#69b200" bold="true"/>
  <style name="rubyonrails:special-variable" foreground="#308095" bold="true"/>
  <style name="rubyonrails:predefined-variable" foreground="#39a0ba" background="#eaf5f7"/>
  <style name="rubyonrails:global-variable" foreground="#308095" bold="true"/>
  <style name="rubyonrails:instance-variable" foreground="#308095" bold="true"/>
  <style name="rubyonrails:class-variable" foreground="#308095" bold="true"/>
  <style name="rubyonrails:operators" foreground="#000000" background="#fefcfd" bold="true"/>
  <style name="rubyonrails:symbol" />
  <style name="rubyonrails:rails" foreground="#308095" bold="true"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" />
  <style name="diff:removed-line" />
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" />
  <style name="diff:diff-file" foreground="#000000" background="#fefcfd" bold="true"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#308095" bold="true"/>
  <style name="css:known-property-values" />
  <style name="css:others-2" foreground="#308095" bold="true"/>
  <style name="css:others-3"  foreground="#308095" bold="true"/>

  <!-- XML Styles -->
  <style name="xml:attribute-name" foreground="#ffffff" background="#69b200" bold="true"/>
  <style name="xml:element-name" foreground="#39a0ba" background="#eaf5f7"/>
  <style name="xml:attribute-value" foreground="#69b200" background="#fffef8"/>
  <style name="xml:namespace" />
  <style name="xml:entity" foreground="#308095" bold="true"/>
  <style name="html:script" foreground="#308095" bold="true"/>

</style-scheme>


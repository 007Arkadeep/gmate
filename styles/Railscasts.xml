<style-scheme id="railscasts" _name="Railscasts" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#545d73"/>
  <style name="text"              foreground="#e6e1dc" background="#2b2b2b"/>
  <style name="cursor"            foreground="#ffffff"/>
  <style name="current-line"      background="#333435"/>
  <style name="line-numbers"      foreground="#e6e1dc" background="#2b2b2b"/>
  <style name="draw-spaces"       foreground="#363636"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#bc9458" italic="true"/>
  <style name="def:shebang" foreground="#bc9458" italic="true"/>
  <style name="def:doc-comment-element" foreground="#bc9458" italic="true"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#da4939"/>
  <style name="def:string" foreground="#a5c261"/>
  <style name="def:character" foreground="#a5c261"/>
  <style name="def:special-char" foreground="#e6e1dc"/>
  <style name="def:special-constant" foreground="#6e9cbe"/>
  <style name="def:instance-variable" foreground="#d0d0ff"/>
  <style name="def:boolean" foreground="#6e9cbe"/>
  <style name="def:number" foreground="#a5c261"/>
  <style name="def:base-n-integer" foreground="#a5c261"/>
  <style name="def:decimal" foreground="#a5c261"/>
  <style name="def:complex" foreground="#a5c261"/>
  <style name="def:floating-point" foreground="#a5c261"/>
  <style name="def:keyword" foreground="#cc7833"/>
  <style name="def:builtin" foreground="#ffc66d"/>
  <style name="def:variable" foreground="#d0d0ff"/>
  <style name="def:class-variable" foreground="#d0d0ff"/>
  <style name="def:identifier" foreground="#d0d0ff"/>
  <style name="def:statement" foreground="#da4939"/>
  <style name="def:type" foreground="#da4939"/>
  <style name="def:error" foreground="#ffffff" background="#990000"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#d0d0ff"/>
  <style name="def:preprocessor" foreground="#cc7833"/>
  <style name="def:specials" foreground="#ffffff"/>
  <!--style name="def:reserved"  foreground="#ffffff" background="#990000"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#ffffff"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#da4939"/>
  <style name="ruby:regex" foreground="#a5c261"/>
  <style name="ruby:class-definition" foreground="#ffffff"/>
  <style name="ruby:simple-interpolation" foreground="#519f50"/>
  <style name="ruby:complex-interpolation" foreground="#519f50"/>
  <style name="ruby:function-definition" foreground="#ffc66d"/>
  <style name="ruby:special-variable" foreground="#d0d0ff"/>
  <style name="ruby:predefined-variable" foreground="#e8bf6a"/>
  <style name="ruby:global-variable" foreground="#d0d0ff"/>
  <style name="ruby:instance-variable" foreground="#d0d0ff"/>
  <style name="ruby:class-variable" foreground="#d0d0ff"/>
  <style name="ruby:operators" foreground="#cc7833"/>
  <style name="ruby:symbol" foreground="#6d9cbe"/>

  <!-- YAML Styles -->
  <style name="yaml:inner-key" foreground="#6d9cbe"/>
  <style name="yaml:scalar-value" foreground="#a5c261"/>

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#da4939"/>
  <style name="rubyonrails:regex" foreground="#a5c261"/>
  <style name="rubyonrails:class-definition" foreground="#ffffff"/>
  <style name="rubyonrails:simple-interpolation" foreground="#519f50"/>
  <style name="rubyonrails:complex-interpolation" foreground="#519f50"/>
  <style name="rubyonrails:function-definition" foreground="#ffc66d"/>
  <style name="rubyonrails:special-variable" foreground="#d0d0ff"/>
  <style name="rubyonrails:predefined-variable" foreground="#e8bf6a"/>
  <style name="rubyonrails:global-variable" foreground="#d0d0ff"/>
  <style name="rubyonrails:instance-variable" foreground="#d0d0ff"/>
  <style name="rubyonrails:class-variable" foreground="#d0d0ff"/>
  <style name="rubyonrails:operators" foreground="#cc7833"/>
  <style name="rubyonrails:symbol" foreground="#6d9cbe"/>
  <style name="rubyonrails:rails" foreground="#da4939"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" foreground="#e6e1dc" background="#144212"/>
  <style name="diff:removed-line" foreground="#e6e1dc" background="#660000"/>
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" foreground="#6d9cbe"/>
  <style name="diff:diff-file" foreground="#cc7833"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#d0d0ff"/>
  <style name="css:known-property-values" foreground="#6d9cbe"/>
  <style name="css:others-2" foreground="#ffffff"/>
  <style name="css:others-3"  foreground="#ffffff"/>

  <!-- XML Styles -->
  <style name="xml:element-name" foreground="#e8bf6a"/>
  <style name="xml:attribute-name" foreground="#e8bf6a"/>
  <style name="xml:attribute-value" foreground="#a5c261"/>
  <style name="xml:processing-instruction" foreground="#e8bf6a"/>
  <style name="xml:namespace" foreground="#6d9cbe"/>
  <style name="xml:entity" foreground="#da4939"/>
  <style name="html:script" foreground="#ffffff"/>

</style-scheme>


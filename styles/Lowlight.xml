<style-scheme id="lowlight" _name="Lowlight" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#292b32"/>
  <style name="text"              foreground="#cacaca" background="#1e1e1e"/>
  <style name="cursor"            foreground="#595959"/>
  <style name="current-line"      background="#1b1b1b"/>
  <style name="line-numbers"      foreground="#cacaca" background="#1e1e1e"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#3c5636" background="#1c1f1b" italic="true"/>
  <style name="def:shebang" foreground="#3c5636" background="#1c1f1b" italic="true"/>
  <style name="def:doc-comment-element" foreground="#3c5636" background="#1c1f1b" italic="true"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#e66c29" bold="true"/>
  <style name="def:string" foreground="#a57c5c"/>
  <style name="def:character" foreground="#a57c5c"/>
  <style name="def:special-char" foreground="#cacaca"/>
  <style name="def:special-constant" foreground="#e66c29"/>
  <style name="def:instance-variable" foreground="#77acb0" bold="true"/>
  <style name="def:boolean" foreground="#e66c29"/>
  <style name="def:number" foreground="#e66c29"/>
  <style name="def:base-n-integer" foreground="#e66c29"/>
  <style name="def:decimal" foreground="#e66c29"/>
  <style name="def:complex" foreground="#e66c29"/>
  <style name="def:floating-point" foreground="#e66c29"/>
  <style name="def:keyword" foreground="#8f8d17"/>
  <style name="def:builtin" foreground="#2eb43b"/>
  <style name="def:variable" foreground="#77acb0" bold="true"/>
  <style name="def:class-variable" foreground="#77acb0" bold="true"/>
  <style name="def:identifier" foreground="#77acb0" bold="true"/>
  <style name="def:statement" foreground="#e66c29" bold="true"/>
  <style name="def:type" foreground="#e66c29" bold="true"/>
  <style name="def:error" foreground="#a2a2a2" background="#351d18" underline="true"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#77acb0" bold="true"/>
  <style name="def:preprocessor" foreground="#8f8d17"/>
  <style name="def:specials" foreground="#2eb43b"/>
  <!--style name="def:reserved"  foreground="#a2a2a2" background="#351d18" underline="true"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#2eb43b"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#e66c29" bold="true"/>
  <style name="ruby:regex" foreground="#e3965e"/>
  <style name="ruby:class-definition" foreground="#2eb43b"/>
  <style name="ruby:simple-interpolation" foreground="#f5e76b"/>
  <style name="ruby:complex-interpolation" foreground="#f5e76b"/>
  <style name="ruby:function-definition" foreground="#2eb43b"/>
  <style name="ruby:special-variable" foreground="#77acb0" bold="true"/>
  <style name="ruby:predefined-variable" foreground="#495573"/>
  <style name="ruby:global-variable" foreground="#77acb0" bold="true"/>
  <style name="ruby:instance-variable" foreground="#77acb0" bold="true"/>
  <style name="ruby:class-variable" foreground="#77acb0" bold="true"/>
  <style name="ruby:operators" foreground="#8f8d17"/>
  <style name="ruby:symbol" foreground="#e66c29"/>

  <!-- YAML Styles -->
  <style name="yaml:inner-key" foreground="#e66c29"/>
  <style name="yaml:scalar-value" foreground="#a57c5c"/>

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#e66c29" bold="true"/>
  <style name="rubyonrails:regex" foreground="#e3965e"/>
  <style name="rubyonrails:class-definition" foreground="#2eb43b"/>
  <style name="rubyonrails:simple-interpolation" foreground="#f5e76b"/>
  <style name="rubyonrails:complex-interpolation" foreground="#f5e76b"/>
  <style name="rubyonrails:function-definition" foreground="#2eb43b"/>
  <style name="rubyonrails:special-variable" foreground="#77acb0" bold="true"/>
  <style name="rubyonrails:predefined-variable" foreground="#495573"/>
  <style name="rubyonrails:global-variable" foreground="#77acb0" bold="true"/>
  <style name="rubyonrails:instance-variable" foreground="#77acb0" bold="true"/>
  <style name="rubyonrails:class-variable" foreground="#77acb0" bold="true"/>
  <style name="rubyonrails:operators" foreground="#8f8d17"/>
  <style name="rubyonrails:symbol" foreground="#e66c29"/>
  <style name="rubyonrails:rails" foreground="#2f8996"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" foreground="#f8f8f8" background="#121d0c" bold="true"/>
  <style name="diff:removed-line" foreground="#f8f8f8" background="#2a0205" italic="true"/>
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" foreground="#e66c29"/>
  <style name="diff:diff-file" foreground="#8f8d17"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#77acb0" bold="true"/>
  <style name="css:known-property-values" foreground="#e66c29"/>
  <style name="css:others-2" foreground="#2eb43b"/>
  <style name="css:others-3"  foreground="#2eb43b"/>

  <!-- XML Styles -->
  <style name="xml:element-name" foreground="#baa827"/>
  <style name="xml:attribute-name" foreground="#b06520"/>
  <style name="xml:attribute-value" foreground="#a57c5c"/>
  <style name="xml:processing-instruction" foreground="#495573"/>
  <style name="xml:namespace" foreground="#e66c29"/>
  <style name="xml:entity" foreground="#e66c29" bold="true"/>
  <style name="html:script" foreground="#2eb43b"/>

</style-scheme>


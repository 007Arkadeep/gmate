<style-scheme id="blackpearlii" _name="Black Pearl II" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#262626"/>
  <style name="text"              foreground="#ffffff" background="#000000"/>
  <style name="cursor"            foreground="#ffffff"/>
  <style name="current-line"      background="#0d0d0d"/>
  <style name="line-numbers"      foreground="#ffffff" background="#000000"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#428bdd" italic="true"/>
  <style name="def:shebang" foreground="#428bdd" italic="true"/>
  <style name="def:doc-comment-element" foreground="#428bdd" italic="true"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#d0ff7e"/>
  <style name="def:string" foreground="#cc66ff"/>
  <style name="def:character" foreground="#cc66ff"/>
  <style name="def:special-char" foreground="#ffffff"/>
  <style name="def:special-constant" foreground="#80d500" bold="true"/>
  <style name="def:instance-variable" foreground="#d0ff7e"/>
  <style name="def:boolean" foreground="#80d500" bold="true"/>
  <style name="def:number" foreground="#eddd5a"/>
  <style name="def:base-n-integer" foreground="#eddd5a"/>
  <style name="def:decimal" foreground="#eddd5a"/>
  <style name="def:complex" foreground="#eddd5a"/>
  <style name="def:floating-point" foreground="#eddd5a"/>
  <style name="def:keyword" foreground="#ff9d00"/>
  <style name="def:builtin" foreground="#ffffff" bold="true"/>
  <style name="def:variable" foreground="#d0ff7e"/>
  <style name="def:class-variable" foreground="#d0ff7e"/>
  <style name="def:identifier" foreground="#d0ff7e"/>
  <style name="def:statement" foreground="#d0ff7e"/>
  <style name="def:type" foreground="#d0ff7e"/>
  <style name="def:error"  background="#670000" bold="true"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#d0ff7e"/>
  <style name="def:preprocessor" foreground="#ff9d00"/>
  <style name="def:specials" foreground="#d0ff7e"/>
  <!--style name="def:reserved"   background="#670000" bold="true"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#d0ff7e"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#d0ff7e"/>
  <style name="ruby:regex" foreground="#ca4344"/>
  <style name="ruby:class-definition" foreground="#d0ff7e"/>
  <style name="ruby:simple-interpolation" foreground="#ff2837" bold="true"/>
  <style name="ruby:complex-interpolation" foreground="#ff2837" bold="true"/>
  <style name="ruby:function-definition" foreground="#ffffff" bold="true"/>
  <style name="ruby:special-variable" foreground="#d0ff7e"/>
  <style name="ruby:predefined-variable" foreground="#ffffff" bold="true"/>
  <style name="ruby:global-variable" foreground="#d0ff7e"/>
  <style name="ruby:instance-variable" foreground="#d0ff7e"/>
  <style name="ruby:class-variable" foreground="#d0ff7e"/>
  <style name="ruby:operators" foreground="#ff9d00"/>
  <style name="ruby:symbol" foreground="#66ccff"/>

  <!-- YAML Styles -->
  <style name="yaml:inner-key" foreground="#66ccff"/>
  <style name="yaml:scalar-value" foreground="#cc66ff"/>

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#d0ff7e"/>
  <style name="rubyonrails:regex" foreground="#ca4344"/>
  <style name="rubyonrails:class-definition" foreground="#d0ff7e"/>
  <style name="rubyonrails:simple-interpolation" foreground="#ff2837" bold="true"/>
  <style name="rubyonrails:complex-interpolation" foreground="#ff2837" bold="true"/>
  <style name="rubyonrails:function-definition" foreground="#ffffff" bold="true"/>
  <style name="rubyonrails:special-variable" foreground="#d0ff7e"/>
  <style name="rubyonrails:predefined-variable" foreground="#ffffff" bold="true"/>
  <style name="rubyonrails:global-variable" foreground="#d0ff7e"/>
  <style name="rubyonrails:instance-variable" foreground="#d0ff7e"/>
  <style name="rubyonrails:class-variable" foreground="#d0ff7e"/>
  <style name="rubyonrails:operators" foreground="#ff9d00"/>
  <style name="rubyonrails:symbol" foreground="#66ccff"/>
  <style name="rubyonrails:rails" foreground="#8aa6c1"/>

  <!-- Diff Settings -->
  <style name="diff:added-line"  background="#253b22"/>
  <style name="diff:removed-line"  background="#420e09"/>
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" foreground="#66ccff"/>
  <style name="diff:diff-file" foreground="#ff9d00"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#d0ff7e"/>
  <style name="css:known-property-values" foreground="#66ccff"/>
  <style name="css:others-2" foreground="#d0ff7e"/>
  <style name="css:others-3"  foreground="#d0ff7e"/>

  <!-- XML Styles -->
  <style name="xml:element-name" foreground="#ffffff" bold="true"/>
  <style name="xml:attribute-name" foreground="#ffffff"/>
  <style name="xml:attribute-value" foreground="#cc66ff"/>
  <style name="xml:processing-instruction" foreground="#ffffff" bold="true"/>
  <style name="xml:namespace" foreground="#66ccff"/>
  <style name="xml:entity" foreground="#d0ff7e"/>
  <style name="html:script" foreground="#d0ff7e"/>

</style-scheme>


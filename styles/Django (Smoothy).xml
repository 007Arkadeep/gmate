<style-scheme id="djangosmoothy" _name="Django (Smoothy)" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#336442"/>
  <style name="text"              foreground="#f8f8f8" background="#245032"/>
  <style name="cursor"            foreground="#336442"/>
  <style name="current-line"      background="#193823"/>
  <style name="line-numbers"      foreground="#f8f8f8" background="#245032"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#fd6209"/>
  <style name="def:shebang" foreground="#fd6209"/>
  <style name="def:doc-comment-element" foreground="#fd6209"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#9cf340" bold="true" italic="true"/>
  <style name="def:string" foreground="#e1ffb8" italic="true"/>
  <style name="def:character" foreground="#e1ffb8" italic="true"/>
  <style name="def:special-char" foreground="#f8f8f8"/>
  <style name="def:special-constant" foreground="#9cf340" bold="true" italic="true"/>
  <style name="def:instance-variable" foreground="#fb9a4b" bold="true" italic="true"/>
  <style name="def:boolean" foreground="#9cf340" bold="true" italic="true"/>
  <style name="def:number" foreground="#9cf340" bold="true" italic="true"/>
  <style name="def:base-n-integer" foreground="#9cf340" bold="true" italic="true"/>
  <style name="def:decimal" foreground="#9cf340" bold="true" italic="true"/>
  <style name="def:complex" foreground="#9cf340" bold="true" italic="true"/>
  <style name="def:floating-point" foreground="#9cf340" bold="true" italic="true"/>
  <style name="def:keyword" foreground="#96dd3b"/>
  <style name="def:builtin" foreground="#fec758"/>
  <style name="def:variable" foreground="#fb9a4b" bold="true" italic="true"/>
  <style name="def:class-variable" foreground="#fb9a4b" bold="true" italic="true"/>
  <style name="def:identifier" foreground="#fb9a4b" bold="true" italic="true"/>
  <style name="def:statement" foreground="#9cf340" bold="true" italic="true"/>
  <style name="def:type" foreground="#9cf340" bold="true" italic="true"/>
  <style name="def:error"  background="#94da3a"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#fb9a4b" bold="true" italic="true"/>
  <style name="def:preprocessor" foreground="#96dd3b"/>
  <style name="def:specials" foreground="#fec758"/>
  <!--style name="def:reserved"   background="#94da3a"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#fec758"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#9cf340" bold="true" italic="true"/>
  <style name="ruby:regex" foreground="#ffb454"/>
  <style name="ruby:class-definition" foreground="#fec758"/>
  <style name="ruby:simple-interpolation" foreground="#ffe862" bold="true" italic="true"/>
  <style name="ruby:complex-interpolation" foreground="#ffe862" bold="true" italic="true"/>
  <style name="ruby:function-definition" foreground="#fec758"/>
  <style name="ruby:special-variable" foreground="#fb9a4b" bold="true" italic="true"/>
  <style name="ruby:predefined-variable" foreground="#e8e8e7"/>
  <style name="ruby:global-variable" foreground="#fb9a4b" bold="true" italic="true"/>
  <style name="ruby:instance-variable" foreground="#fb9a4b" bold="true" italic="true"/>
  <style name="ruby:class-variable" foreground="#fb9a4b" bold="true" italic="true"/>
  <style name="ruby:operators" foreground="#96dd3b"/>
  <style name="ruby:symbol" foreground="#9cf340" bold="true" italic="true"/>

  <!-- YAML Styles -->
  <style name="yaml:inner-key" foreground="#9cf340" bold="true" italic="true"/>
  <style name="yaml:scalar-value" foreground="#e1ffb8" italic="true"/>

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#9cf340" bold="true" italic="true"/>
  <style name="rubyonrails:regex" foreground="#ffb454"/>
  <style name="rubyonrails:class-definition" foreground="#fec758"/>
  <style name="rubyonrails:simple-interpolation" foreground="#ffe862" bold="true" italic="true"/>
  <style name="rubyonrails:complex-interpolation" foreground="#ffe862" bold="true" italic="true"/>
  <style name="rubyonrails:function-definition" foreground="#fec758"/>
  <style name="rubyonrails:special-variable" foreground="#fb9a4b" bold="true" italic="true"/>
  <style name="rubyonrails:predefined-variable" foreground="#e8e8e7"/>
  <style name="rubyonrails:global-variable" foreground="#fb9a4b" bold="true" italic="true"/>
  <style name="rubyonrails:instance-variable" foreground="#fb9a4b" bold="true" italic="true"/>
  <style name="rubyonrails:class-variable" foreground="#fb9a4b" bold="true" italic="true"/>
  <style name="rubyonrails:operators" foreground="#96dd3b"/>
  <style name="rubyonrails:symbol" foreground="#9cf340" bold="true" italic="true"/>
  <style name="rubyonrails:rails" foreground="#9df39f"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" foreground="#41a83e" background="#9bf199"/>
  <style name="diff:removed-line" foreground="#ec9799" background="#fedcdd"/>
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" foreground="#9cf340" bold="true" italic="true"/>
  <style name="diff:diff-file" foreground="#96dd3b"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#fb9a4b" bold="true" italic="true"/>
  <style name="css:known-property-values" foreground="#9cf340" bold="true" italic="true"/>
  <style name="css:others-2" foreground="#fec758"/>
  <style name="css:others-3"  foreground="#fec758"/>

  <!-- XML Styles -->
  <style name="xml:element-name" foreground="#e8e8e7"/>
  <style name="xml:attribute-name" foreground="#e8e8e7"/>
  <style name="xml:attribute-value" foreground="#e1ffb8" italic="true"/>
  <style name="xml:processing-instruction" foreground="#e8e8e7"/>
  <style name="xml:namespace" foreground="#9cf340" bold="true" italic="true"/>
  <style name="xml:entity" foreground="#9cf340" bold="true" italic="true"/>
  <style name="html:script" foreground="#fec758"/>

</style-scheme>


<style-scheme id="travisjeffery" _name="Travis Jeffery" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#87074a"/>
  <style name="text"              foreground="#a79ea8" background="#0f1014"/>
  <style name="cursor"            foreground="#a7a7a7"/>
  <style name="current-line"      background="#16171b"/>
  <style name="line-numbers"      foreground="#a79ea8" background="#0f1014"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#6f6970" italic="true"/>
  <style name="def:shebang" foreground="#6f6970" italic="true"/>
  <style name="def:doc-comment-element" foreground="#6f6970" italic="true"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#ffffff" bold="true"/>
  <style name="def:string" foreground="#4dfcff" bold="true"/>
  <style name="def:character" foreground="#4dfcff" bold="true"/>
  <style name="def:special-char" foreground="#a79ea8"/>
  <style name="def:special-constant" foreground="#ffffff" bold="true"/>
  <style name="def:instance-variable" foreground="#aeb7ec" bold="true"/>
  <style name="def:boolean" foreground="#ffffff" bold="true"/>
  <style name="def:number" foreground="#ffffff" bold="true"/>
  <style name="def:base-n-integer" foreground="#ffffff" bold="true"/>
  <style name="def:decimal" foreground="#ffffff" bold="true"/>
  <style name="def:complex" foreground="#ffffff" bold="true"/>
  <style name="def:floating-point" foreground="#ffffff" bold="true"/>
  <style name="def:keyword" foreground="#ff66ff" background="#3a8789" bold="true"/>
  <style name="def:builtin" foreground="#66ffcc" background="#07484a" bold="true" underline="true"/>
  <style name="def:variable" foreground="#aeb7ec" bold="true"/>
  <style name="def:class-variable" foreground="#aeb7ec" bold="true"/>
  <style name="def:identifier" foreground="#aeb7ec" bold="true"/>
  <style name="def:statement" foreground="#ffffff" bold="true"/>
  <style name="def:type" foreground="#ffffff" bold="true"/>
  <style name="def:error" foreground="#f8f8f8" background="#442545"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#aeb7ec" bold="true"/>
  <style name="def:preprocessor" foreground="#ff66ff" background="#3a8789" bold="true"/>
  <style name="def:specials" foreground="#66ffcc" background="#07484a" bold="true" underline="true"/>
  <!--style name="def:reserved"  foreground="#f8f8f8" background="#442545"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#66ffcc" background="#07484a" bold="true" underline="true"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#ffffff" bold="true"/>
  <style name="ruby:regex" foreground="#e9c062"/>
  <style name="ruby:class-definition" foreground="#66ffcc" background="#07484a" bold="true" underline="true"/>
  <style name="ruby:simple-interpolation" foreground="#70ae8d" bold="true"/>
  <style name="ruby:complex-interpolation" foreground="#70ae8d" bold="true"/>
  <style name="ruby:function-definition" foreground="#66ffcc" background="#07484a" bold="true" underline="true"/>
  <style name="ruby:special-variable" foreground="#aeb7ec" bold="true"/>
  <style name="ruby:predefined-variable" foreground="#7c7f81"/>
  <style name="ruby:global-variable" foreground="#aeb7ec" bold="true"/>
  <style name="ruby:instance-variable" foreground="#aeb7ec" bold="true"/>
  <style name="ruby:class-variable" foreground="#aeb7ec" bold="true"/>
  <style name="ruby:operators" foreground="#ff66ff" background="#3a8789" bold="true"/>
  <style name="ruby:symbol" foreground="#ff8000" bold="true"/>

  <!-- YAML Styles -->
  <style name="yaml:inner-key" foreground="#ff8000" bold="true"/>
  <style name="yaml:scalar-value" foreground="#4dfcff" bold="true"/>

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#ffffff" bold="true"/>
  <style name="rubyonrails:regex" foreground="#e9c062"/>
  <style name="rubyonrails:class-definition" foreground="#66ffcc" background="#07484a" bold="true" underline="true"/>
  <style name="rubyonrails:simple-interpolation" foreground="#70ae8d" bold="true"/>
  <style name="rubyonrails:complex-interpolation" foreground="#70ae8d" bold="true"/>
  <style name="rubyonrails:function-definition" foreground="#66ffcc" background="#07484a" bold="true" underline="true"/>
  <style name="rubyonrails:special-variable" foreground="#aeb7ec" bold="true"/>
  <style name="rubyonrails:predefined-variable" foreground="#7c7f81"/>
  <style name="rubyonrails:global-variable" foreground="#aeb7ec" bold="true"/>
  <style name="rubyonrails:instance-variable" foreground="#aeb7ec" bold="true"/>
  <style name="rubyonrails:class-variable" foreground="#aeb7ec" bold="true"/>
  <style name="rubyonrails:operators" foreground="#ff66ff" background="#3a8789" bold="true"/>
  <style name="rubyonrails:symbol" foreground="#ff8000" bold="true"/>
  <style name="rubyonrails:rails" foreground="#9b9cbf" bold="true"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" foreground="#f8f8f8" background="#253b22"/>
  <style name="diff:removed-line" foreground="#f8f8f8" background="#420e09"/>
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" foreground="#ff8000" bold="true"/>
  <style name="diff:diff-file" foreground="#ff66ff" background="#3a8789" bold="true"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#aeb7ec" bold="true"/>
  <style name="css:known-property-values" foreground="#ff8000" bold="true"/>
  <style name="css:others-2" foreground="#66ffcc" background="#07484a" bold="true" underline="true"/>
  <style name="css:others-3"  foreground="#66ffcc" background="#07484a" bold="true" underline="true"/>

  <!-- XML Styles -->
  <style name="xml:element-name" foreground="#7c7f81"/>
  <style name="xml:attribute-name" foreground="#7c7f81" background="#0a585a" bold="true"/>
  <style name="xml:attribute-value" foreground="#4dfcff" bold="true"/>
  <style name="xml:processing-instruction" foreground="#7c7f81"/>
  <style name="xml:namespace" foreground="#ff8000" bold="true"/>
  <style name="xml:entity" foreground="#ffffff" bold="true"/>
  <style name="html:script" foreground="#66ffcc" background="#07484a" bold="true" underline="true"/>

</style-scheme>


<style-scheme id="forlatex" _name="ForLaTeX" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#bad6fd"/>
  <style name="text"              foreground="#66ccff" background="#000000"/>
  <style name="cursor"            foreground="#ff8000"/>
  <style name="current-line"      background="#7f7f7f"/>
  <style name="line-numbers"      foreground="#66ccff" background="#000000"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#66ff66" italic="true"/>
  <style name="def:shebang" foreground="#66ff66" italic="true"/>
  <style name="def:doc-comment-element" foreground="#66ff66" italic="true"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#ffff00"/>
  <style name="def:string" foreground="#ff66ff"/>
  <style name="def:character" foreground="#ff66ff"/>
  <style name="def:special-char" foreground="#66ccff"/>
  <style name="def:special-constant" />
  <style name="def:instance-variable" />
  <style name="def:boolean" />
  <style name="def:number" />
  <style name="def:base-n-integer" />
  <style name="def:decimal" />
  <style name="def:complex" />
  <style name="def:floating-point" />
  <style name="def:keyword" foreground="#999999"/>
  <style name="def:builtin" />
  <style name="def:variable" />
  <style name="def:class-variable" />
  <style name="def:identifier" />
  <style name="def:statement" foreground="#ffff00"/>
  <style name="def:type" foreground="#ffff00"/>
  <style name="def:error" />
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" />
  <style name="def:preprocessor" foreground="#999999"/>
  <style name="def:specials" foreground="#ffff00"/>
  <!--style name="def:reserved"  />
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#ffff00"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#ffff00"/>
  <style name="ruby:regex" foreground="#ff66ff"/>
  <style name="ruby:class-definition" foreground="#ffff00"/>
  <style name="ruby:simple-interpolation" foreground="#ff66ff"/>
  <style name="ruby:complex-interpolation" foreground="#ff66ff"/>
  <style name="ruby:function-definition" />
  <style name="ruby:special-variable" />
  <style name="ruby:predefined-variable" />
  <style name="ruby:global-variable" />
  <style name="ruby:instance-variable" />
  <style name="ruby:class-variable" />
  <style name="ruby:operators" foreground="#999999"/>
  <style name="ruby:symbol" foreground="#ffff00"/>

  <!-- YAML Styles -->
  <style name="yaml:inner-key" foreground="#ffff00"/>
  <style name="yaml:scalar-value" foreground="#ff66ff"/>

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#ffff00"/>
  <style name="rubyonrails:regex" foreground="#ff66ff"/>
  <style name="rubyonrails:class-definition" foreground="#ffff00"/>
  <style name="rubyonrails:simple-interpolation" foreground="#ff66ff"/>
  <style name="rubyonrails:complex-interpolation" foreground="#ff66ff"/>
  <style name="rubyonrails:function-definition" />
  <style name="rubyonrails:special-variable" />
  <style name="rubyonrails:predefined-variable" />
  <style name="rubyonrails:global-variable" />
  <style name="rubyonrails:instance-variable" />
  <style name="rubyonrails:class-variable" />
  <style name="rubyonrails:operators" foreground="#999999"/>
  <style name="rubyonrails:symbol" foreground="#ffff00"/>
  <style name="rubyonrails:rails" foreground="#ffcc66"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" />
  <style name="diff:removed-line" />
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" foreground="#ffff00"/>
  <style name="diff:diff-file" foreground="#999999"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" />
  <style name="css:known-property-values" foreground="#ffff00"/>
  <style name="css:others-2" foreground="#ffff00"/>
  <style name="css:others-3"  foreground="#ffff00"/>

  <!-- XML Styles -->
  <style name="xml:element-name" />
  <style name="xml:attribute-name" />
  <style name="xml:attribute-value" foreground="#ff66ff"/>
  <style name="xml:processing-instruction" />
  <style name="xml:namespace" foreground="#ffff00"/>
  <style name="xml:entity" foreground="#ffff00"/>
  <style name="html:script" foreground="#ffff00"/>

</style-scheme>


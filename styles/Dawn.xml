<style-scheme id="dawn" _name="Dawn" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#b9cafa"/>
  <style name="text"              foreground="#080808" background="#f9f9f9"/>
  <style name="cursor"            foreground="#000000"/>
  <style name="current-line"      background="#dfe6f0"/>
  <style name="line-numbers"      foreground="#080808" background="#f9f9f9"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#5a525f" italic="true"/>
  <style name="def:shebang" foreground="#5a525f" italic="true"/>
  <style name="def:doc-comment-element" foreground="#5a525f" italic="true"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#234a97"/>
  <style name="def:string" />
  <style name="def:character" />
  <style name="def:special-char" foreground="#080808"/>
  <style name="def:special-constant" foreground="#811f24" bold="true"/>
  <style name="def:instance-variable" foreground="#234a97"/>
  <style name="def:boolean" foreground="#811f24" bold="true"/>
  <style name="def:number" foreground="#811f24" bold="true"/>
  <style name="def:base-n-integer" foreground="#811f24" bold="true"/>
  <style name="def:decimal" foreground="#811f24" bold="true"/>
  <style name="def:complex" foreground="#811f24" bold="true"/>
  <style name="def:floating-point" foreground="#811f24" bold="true"/>
  <style name="def:keyword" foreground="#794938"/>
  <style name="def:builtin" foreground="#bf4f24"/>
  <style name="def:variable" foreground="#234a97"/>
  <style name="def:class-variable" foreground="#234a97"/>
  <style name="def:identifier" foreground="#234a97"/>
  <style name="def:statement" foreground="#234a97"/>
  <style name="def:type" foreground="#234a97"/>
  <style name="def:error" background="#C80000" foreground="#F0EA20"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#234a97"/>
  <style name="def:preprocessor" foreground="#794938"/>
  <style name="def:specials" foreground="#bf4f24"/>
  <!--style name="def:reserved"  background="#C80000" foreground="#F0EA20"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#bf4f24"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#234a97"/>
  <style name="ruby:regex" foreground="#cf5628"/>
  <style name="ruby:class-definition" foreground="#bf4f24"/>
  <style name="ruby:simple-interpolation" foreground="#811f24" bold="true"/>
  <style name="ruby:complex-interpolation" foreground="#811f24" bold="true"/>
  <style name="ruby:function-definition" foreground="#bf4f24"/>
  <style name="ruby:special-variable" foreground="#234a97"/>
  <style name="ruby:predefined-variable" />
  <style name="ruby:global-variable" foreground="#234a97"/>
  <style name="ruby:instance-variable" foreground="#234a97"/>
  <style name="ruby:class-variable" foreground="#234a97"/>
  <style name="ruby:operators" foreground="#794938"/>
  <style name="ruby:symbol" foreground="#811f24" bold="true"/>

  <!-- YAML Styles -->
  <style name="yaml:inner-key" foreground="#811f24" bold="true"/>
  <style name="yaml:scalar-value" />

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#234a97"/>
  <style name="rubyonrails:regex" foreground="#cf5628"/>
  <style name="rubyonrails:class-definition" foreground="#bf4f24"/>
  <style name="rubyonrails:simple-interpolation" foreground="#811f24" bold="true"/>
  <style name="rubyonrails:complex-interpolation" foreground="#811f24" bold="true"/>
  <style name="rubyonrails:function-definition" foreground="#bf4f24"/>
  <style name="rubyonrails:special-variable" foreground="#234a97"/>
  <style name="rubyonrails:predefined-variable" />
  <style name="rubyonrails:global-variable" foreground="#234a97"/>
  <style name="rubyonrails:instance-variable" foreground="#234a97"/>
  <style name="rubyonrails:class-variable" foreground="#234a97"/>
  <style name="rubyonrails:operators" foreground="#794938"/>
  <style name="rubyonrails:symbol" foreground="#811f24" bold="true"/>
  <style name="rubyonrails:rails" foreground="#234a97"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" />
  <style name="diff:removed-line" foreground="#59140e"/>
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" foreground="#811f24" bold="true"/>
  <style name="diff:diff-file" foreground="#794938"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#234a97"/>
  <style name="css:known-property-values" foreground="#811f24" bold="true"/>
  <style name="css:others-2" foreground="#bf4f24"/>
  <style name="css:others-3"  foreground="#bf4f24"/>

  <!-- XML Styles -->
  <style name="xml:attribute-name" foreground="#bf4f24"/>
  <style name="xml:element-name" />
  <style name="xml:attribute-value" />
  <style name="xml:namespace" foreground="#811f24" bold="true"/>
  <style name="xml:entity" foreground="#234a97"/>
  <style name="html:script" foreground="#bf4f24"/>

</style-scheme>


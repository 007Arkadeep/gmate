<style-scheme id="minimaltheme" _name="minimal Theme" version="1.0">
  <author>tm2gtksw2(Alexandre da Silva)</author>
  <_description>GMate Theme - Generated from textmate theme with tm2gtksw2</_description>

  <!-- Global Settings -->
  <style name="selection"         background="#180024"/>
  <style name="text"              foreground="#fcffe0" background="#302d26"/>
  <style name="cursor"            foreground="#ff9900"/>
  <style name="current-line"      background="#232826"/>
  <style name="line-numbers"      foreground="#fcffe0" background="#302d26"/>

  <!-- Bracket Matching -->
  <style name="bracket-match" background="#4C4C4C"/>
  <style name="bracket-mismatch" background="#4C4C4C" bold="true"/>
  <style name="search-match" background="#404040"/>

  <!-- Comments -->
  <style name="def:comment" foreground="#706d5b" italic="true"/>
  <style name="def:shebang" foreground="#706d5b" italic="true"/>
  <style name="def:doc-comment-element" foreground="#706d5b" italic="true"/>

  <!-- Constants and Variables-->
  <style name="def:constant" foreground="#ab631b"/>
  <style name="def:string" />
  <style name="def:character" />
  <style name="def:special-char" foreground="#fcffe0"/>
  <style name="def:special-constant" foreground="#ab631b"/>
  <style name="def:instance-variable" foreground="#d1a796"/>
  <style name="def:boolean" foreground="#ab631b"/>
  <style name="def:number" foreground="#ab631b"/>
  <style name="def:base-n-integer" foreground="#ab631b"/>
  <style name="def:decimal" foreground="#ab631b"/>
  <style name="def:complex" foreground="#ab631b"/>
  <style name="def:floating-point" foreground="#ab631b"/>
  <style name="def:keyword" foreground="#949c8b"/>
  <style name="def:builtin" foreground="#85730b"/>
  <style name="def:variable" foreground="#d1a796"/>
  <style name="def:class-variable" foreground="#d1a796"/>
  <style name="def:identifier" foreground="#d1a796"/>
  <style name="def:statement" foreground="#ab631b"/>
  <style name="def:type" foreground="#ab631b"/>
  <style name="def:error" foreground="#f8f8f8" background="#800f00"/>
  <style name="def:note" background="#F0EA20" foreground="#C80000"/>
  <style name="def:net-address-in-comment" foreground="#d1a796"/>
  <style name="def:preprocessor" foreground="#949c8b"/>
  <style name="def:specials" foreground="#85730b"/>
  <!--style name="def:reserved"  foreground="#f8f8f8" background="#800f00"/>
  <style name="def:operator"/-->
  <style name="def:underlined" underline="true"/>
  <style name="def:function" foreground="#85730b"/>

  <!-- Ruby Styles -->
  <style name="ruby:module-handler" foreground="#ab631b"/>
  <style name="ruby:regex" foreground="#62b58a"/>
  <style name="ruby:class-definition" foreground="#85730b"/>
  <style name="ruby:simple-interpolation" foreground="#53ab4e"/>
  <style name="ruby:complex-interpolation" foreground="#53ab4e"/>
  <style name="ruby:function-definition" foreground="#85730b"/>
  <style name="ruby:special-variable" foreground="#d1a796"/>
  <style name="ruby:predefined-variable" foreground="#babd9c"/>
  <style name="ruby:global-variable" foreground="#d1a796"/>
  <style name="ruby:instance-variable" foreground="#d1a796"/>
  <style name="ruby:class-variable" foreground="#d1a796"/>
  <style name="ruby:operators" foreground="#949c8b"/>
  <style name="ruby:symbol" foreground="#ab631b"/>

  <!-- YAML Styles -->
  <style name="yaml:inner-key" foreground="#ab631b"/>
  <style name="yaml:scalar-value" />

  <!-- Rails Styles -->
  <style name="rubyonrails:module-handler" foreground="#ab631b"/>
  <style name="rubyonrails:regex" foreground="#62b58a"/>
  <style name="rubyonrails:class-definition" foreground="#85730b"/>
  <style name="rubyonrails:simple-interpolation" foreground="#53ab4e"/>
  <style name="rubyonrails:complex-interpolation" foreground="#53ab4e"/>
  <style name="rubyonrails:function-definition" foreground="#85730b"/>
  <style name="rubyonrails:special-variable" foreground="#d1a796"/>
  <style name="rubyonrails:predefined-variable" foreground="#babd9c"/>
  <style name="rubyonrails:global-variable" foreground="#d1a796"/>
  <style name="rubyonrails:instance-variable" foreground="#d1a796"/>
  <style name="rubyonrails:class-variable" foreground="#d1a796"/>
  <style name="rubyonrails:operators" foreground="#949c8b"/>
  <style name="rubyonrails:symbol" foreground="#ab631b"/>
  <style name="rubyonrails:rails" foreground="#9fc28a"/>

  <!-- Diff Settings -->
  <style name="diff:added-line" foreground="#f8f8f8" background="#228000"/>
  <style name="diff:removed-line" foreground="#f8f8f8" background="#800f00"/>
  <style name="diff:location" />
  <style name="diff:ignore" />
  <style name="diff:changed-line" foreground="#ab631b"/>
  <style name="diff:diff-file" foreground="#949c8b"/>
  <style name="diff:special-case" map-to="diff:location"/>

  <!-- SH Styles -->
  <style name="sh:others" map-to="text"/>

  <!-- C Styles -->
  <style name="c:preprocessor" map-to="def:special-char"/>

  <!-- CSS Styles -->
  <style name="css:function" foreground="#d1a796"/>
  <style name="css:known-property-values" foreground="#ab631b"/>
  <style name="css:others-2" foreground="#85730b"/>
  <style name="css:others-3"  foreground="#85730b"/>

  <!-- XML Styles -->
  <style name="xml:element-name" foreground="#babd9c"/>
  <style name="xml:attribute-name" foreground="#ba6912"/>
  <style name="xml:attribute-value" />
  <style name="xml:processing-instruction" foreground="#babd9c"/>
  <style name="xml:namespace" foreground="#ab631b"/>
  <style name="xml:entity" foreground="#ab631b"/>
  <style name="html:script" foreground="#85730b"/>

</style-scheme>

